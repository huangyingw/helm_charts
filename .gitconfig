[remote "helm"]
    url = git@github.com:helm/charts.git
    fetch = +refs/heads/*:refs/remotes/helm/*
[remote "origin"]
    url = git@github.com:huangyingw/helm_charts.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[push]
    remote = origin
[gsync]
    remote = helm
    branch = master
[merge]
    allow = true
[branch "dev"]
    remote = origin
    merge = refs/heads/dev
[branch "master"]
    remote = origin
    merge = refs/heads/master
[gdif]
    ignore = *.gdio.diff
[deploy]
	host = localhost
	path = ~/myproject/../helm/charts
[gdif]
    ignore = stable/kubernetes-dashboard/deploy.sh
    ignore = stable/mariadb/deploy.sh
    ignore = stable/nfs-server-provisioner/deploy.sh
    ignore = stable/wordpress/deploy.sh
